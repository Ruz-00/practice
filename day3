const cart = ["Pizza", "Burger"];

"array indexing"
console.log(cart[0]);
console.log(cart[1]);

"output"
Pizza
Burger

"add to array" // add from end
cart.push("Fries");
cart.push("Spaghetti");

"output"
["Pizza", "Burger", "Fries", "Spaghetti"]

"remove to arrays" // remove from end
cart.pop(); 

"output"
["Pizza", "Burger", "Fries"]
 
